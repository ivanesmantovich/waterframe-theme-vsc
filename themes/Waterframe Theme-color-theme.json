    // semanticTokenColors OVERRIDE tokenColors
    "semanticHighlighting": true,

    "semanticTokenColors": {
        "function": "#9381FF",
        "method": "#9381FF",
        "class": "#9381FF",
        "macro": "#9381FF",

        "variable": "#000000",
        "keyword": "#000000",
        "string": "#000000",
        "operator": "#000000",
        "number": "#000000",
        "enumMember": "#000000",
        "decorator": "#000000",
        "parameter": "#000000",
        "typeParameter": "#000000",
        "property": "#000000",
        "event": "#000000",
        "label": "#000000",

        "struct": "#BFD62E",
        "interface": "#BFD62E",
        "type": "#BFD62E",
        "namespace": "#BFD62E",
        "enum": "#BFD62E",

        "comment": "#CEC7FF",

        "regexp": "#36454F",
    },

	"tokenColors": [
        // Variable
        {
            "scope": "variable",
            "settings": { "foreground": "#000000", "fontStyle": "" }
        },
        // Function
        {
            "scope": "entity.name.function",
            "settings": { "foreground": "#9381FF", "fontStyle": "bold" }
        },
        // Method
        {
            "scope": "entity.name.method",
            "settings": { "foreground": "#9381FF", "fontStyle": "bold" }
        },
        {
            "scope": "entity.name.type",
            "settings": { "foreground": "#BFD62E" }
        },
        {
            "scope": "entity.name.type",
            "settings": { "foreground": "#BFD62E" }
        },
        {
            "scope": "support.type",
            "settings": { "foreground": "#BFD62E" }
        },
        // HTML Tag
        {
            "scope": "entity.name.tag",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        // HTML Tag Attibute Name
        {
            "scope": "entity.other.attribute-name",
            "settings": { "foreground": "#CEC7FF", "fontStyle": "bold" }
        },
        // function, const, let, ...
        {
            "scope": "storage.type",
            "settings": { "foreground": "#000000", "fontStyle": "italic" }
        },
        // Arrow
        {
            "scope": "storage.type.function.arrow",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        // return, for, break, ...
        {
            "scope": "keyword.control",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        // String
        {
            "scope": "string",
            "settings": {"foreground": "#36454f", "fontStyle": "italic" }
        },
        // Comment
        {
            "scope": "comment",
            "settings": { "foreground": "#CEC7FF", "fontStyle": "italic" }
        },
	]
}
